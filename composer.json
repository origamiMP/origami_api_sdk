{
    "name": "origami-mp/origami-api-sdk",
    "version": "v0.1",
    "description": "A lightweight package to interact easily with Origami API.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Origami Marketplace",
            "email": "developers@origami-marketplace.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.9",
        "illuminate/support": "^12.9",
        "nesbot/carbon": "^3.9",
        "firebase/php-jwt": "^6.11",
        "illuminate/validation": "^12.10"
    },
    "require-dev": {
        "laravel/pint": "^1.22",
        "phpstan/phpstan": "^2.1",
        "psy/psysh": "^0.12.8"
    },
    "autoload": {
        "psr-4": {
            "OrigamiMp\\OrigamiApiSdk\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "stable"
}
